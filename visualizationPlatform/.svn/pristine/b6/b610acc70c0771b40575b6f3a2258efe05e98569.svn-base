package androidServer.bean;

public class HeatData {
	public double lng;
	public double lat;
	public int value = 0;

	public HeatData() {

	}

	public HeatData(double lng, double lat, int value) {
		this.lat = lat;
		this.lng = lng;
		this.value = value;
	}

	public double getLng() {
		return lng;
	}

	public void setLng(double lng) {
		this.lng = lng;
	}

	public double getLat() {
		return lat;
	}

	public void setLat(double lat) {
		this.lat = lat;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

}