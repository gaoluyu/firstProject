package androidServer.util;

public class AuthenticateConstant {
	public static class AuthenticationBit {
		// 0000 0001
		public static final int inspect_report_bit = 0x01;
		// 0000 0010
		public static final int inspect_admin_bit = 0x02;
		// 0000 0100
		public static final int cell_modify_bit = 0x04;
		// 0000 1000
		public static final int cell_read_bit = 0x08;
		// 0001 0000
		public static final int device_read_bit = 0x10;
		// 0010 0000
		public static final int device_modify_bit = 0x20;
	}
}
