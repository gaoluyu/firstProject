} 
/**分頁**/
.page {
	margin: 0px 12px;
	float: left
}

.page a {
	margin: 0px 3px;
	border: 1px solid #e7e7eb;
	display: inline-block;
	line-height: 28px;
	padding: 0px 12px;
}

.page a:hover {
	background: #f3f3f3;
}

.page a.current {
	background: #46b8da;
	border: 1px solid #52ACC7;
	color: #fff;
}

/**building**/
#allType {
	height: 15px;
	font-size: 10px;
}
/**上传图片**/
.upload_1 {
	height: 50px;
	position: relative;
}

a.upload_btn {
	width: 130px;
	text-align: center;
	background: #ffab72;
	display: inline-block;
	color: #fff;
	line-height: 36px;
	float: left;
}

a.upload_btn:hover {
	background: #ff8a3a;
}

span.wz {
	font-size: 12px;
	line-height: 18px;
	display: inline-block;
	float: left;
	margin-left: 15px;
	padding-top: 1px;
	color: #888;
}

input.upload_file {
	position: absolute;
	opacity: 0;
	left: 0px;
	top: 0px;
	width: 130px;
	height: 36px;
	cursor: pointer;
}

.pt_con {
	width: 104px;
	float: left;
	margin-right: 10px;
	margin-bottom: 10px;
}

.pt_con p.pt {
	border: 1px solid #e7e7eb;
	margin-bottom: 2px;
	position: relative;
}

.pt_con p.pt img {
	width: 102px;
	height: 102px;
	vertical-align: middle;
}

.pt_con p.pt a.deletePtBtn {
	width: 100%;
	position: absolute;
	top: 0px;
	left: 0px;
	height: 102px;
	background: #000;
	opacity: 0.7;
	text-align: center;
	line-height: 102px;
	color: #fff;
	display: none;
}

.pt_con p.pt:hover a.deletePtBtn {
	display: block;
}

.pt_con .wz {
	position: relative;
}

.pt_con .wz span.compile_btn {
	display: inline-block;
	width: 100%;
	line-height: 32px;
	background: #ececec;
	text-align: center;
}

.pt_con .wz input.compile_file {
	position: absolute;
	opacity: 0;
	left: 0px;
	top: 0px;
	width: 104px;
	height: 32px;
	cursor: pointer;
}
/**定位监测点个数**/
.pointnum {
	float: right;
