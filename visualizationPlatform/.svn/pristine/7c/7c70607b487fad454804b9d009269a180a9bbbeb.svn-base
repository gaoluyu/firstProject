.bodybg {
	font-family: 微软雅黑, verdana, arial, helvetica, sans-serif;
	font-size: 14px;
	line-height: 1.42857143;
	color: #555555;
	background-color: #ffffff;
}

body {
	font-family: "微软雅黑";
	overflow-x: hidden;
}

a {
	color: #2fa4e7;
	text-decoration: none;
}
col-md-10 content{
	min-width:700px;
	max-width:1500px;
}
/**新的导航栏样式**/
.sidebar-nav {
	min-height: 0;
	box-shadow: 0 0 10px #bdbdbd;
	border-radius: 5px;
	margin-bottom: 0;
	padding-bottom: 0;
	max-height: none;
}

.sidebar-nav .nav-header {
	display: block;
	padding: 8px 20px !important;
	font-size: 12px;
	font-weight: bold;
	line-height: 18px;
	color: #999999;
	text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
	text-transform: uppercase;
	margin-left: 0 !important;
	background-color: #eee;
	box-shadow:0px 1px 5px rgba(221, 221, 221, 0.46) inset;
}

.main-menu>li>a {
	border: 1px solid rgba(221, 221, 221, 0.46);
	text-align:center;
	box-shadow:0px 1px 2px rgba(221, 221, 221, 0.46);
}

ul.main-menu li:not (.nav-header ){
	transition: 2s;
}

ul.main-menu li.active {
	margin-left: 20px;
	background-color: #2fa4e7;
	borer-radius: 6px;
	box-shadow:2px 2px 4px raga(221, 221, 221, 0.46);
}

ul.main-menu li.active a, .main-menu li.active a:hover, .main-menu li.active a:focus
	{
	color: #ffffff;
	background-color: #2fa4e7;
	border-radius: 4px;
	text-align:center;
	box-shadow:inset 2px 2px 4px raga(221, 221, 221, 0.46);
}

ul.main-menu li:hover {
	margin-left: 20px;
}

.main-menu>li {
	margin-top: 0 !important;
}

/***标题栏***/
.nav-logo {
	width: 40px;
	height: 40px;
	float: left;
}

@media only screen and (min-width:980px) a , button , input [type=submit] ,
		input [type=button] {
	transition:0.5s;
}

.nav-title {
	float: left;
	padding: 10px 20px 10px;
	margin-left: -20px;
	font-size: 20px;
	font-weight: 600;
	color: #ffffff;
	letter-spacing: 2px;
	text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5)
}

.navbar-bg {
	background-image: -webkit-linear-gradient(#54b4eb, #2fa4e7 60%, #1d9ce5);
	background-image: linear-gradient(#54b4eb, #2fa4e7 60%, #1d9ce5);
	background-repeat: no-repeat;
	border-bottom: 1px solid #178acc;
	filter: none;
	box-shadow: 0 1px 10px rgba(0, 0, 0, 0.1);
	margin-left: -17px;
	margin-right: -17px;
	margin-top: -1px;
	padding-left: 15px;
	padding-right: 15px;
}

.navbar-inner {
	padding-top: 5px;
	padding-bottom: 5px;
	line-height: 30px;
	height: 60px;
}

.red {
	color: #B94846
}

.notification {
	display: block;
	position: absolute;
	top: -5px;
	right: 0px;
	line-height: 16px;
	height: 16px;
	padding: 0 5px;
	font-family: Arial, sans-serif;
	color: white !important;
	text-shadow: 0 1px hsla(0, 0%, 0%, 0.25);
	border-width: 1px;
	border-style: solid;
	border-radius: 10px;
	box-shadow: 0 1px 1px hsla(0, 0%, 0%, 0.08), inset 0 1px
		hsla(0, 100%, 100%, 0.3);
}

.notification.red {
	border-color: #FA5833;
	background-color: #FA603D;
	background-image: -webkit-linear-gradient(top, hsl(11, 95%, 76%),
		hsl(11, 95%, 61%));
}


